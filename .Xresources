! special
*.foreground:   #FFFFFF
*.background:   #000000
*.cursorColor:  #ababab

! black
*.color0:       #000000
*.color8:       #343434

! red
*.color1:       #e92f2f
*.color9:       #e92f2f

! green
*.color2:       #0ed839
*.color10:      #0ed839

! yellow
*.color3:       #dddd13
*.color11:      #dddd13

! blue
*.color4:       #3b48e3
*.color12:      #3b48e3

! magenta
*.color5:       #f996e2
*.color13:      #f996e2

! cyan
*.color6:       #23edda
*.color14:      #23edda

! white
*.color7:       #ababab
*.color15:      #f9f9f9

URxvt.internalBorder: 4

!font
URxvt.font: -*-tamsyn-medium-*-*-*-17-*-*-*-*-*-*-*
!        xft:Adobe Source Han Sans Normal:size=12,\
!	   !xft:Kochi Gothic:antialias=false,\
!       !xft:Baekmuk Gulim
!URxvt.font:     xft:Tamzen:medium:size=12:antialias=false
!URxvt.boldFont: xft:bitstream vera sans mono:bold:size=16:antialias=true
!URxvt.boldFont:     xft:Tamzen:bold:size=12:antialias=false
URxvt.scrollBar: false
!URxvt.letterSpace: 0

!Extensions:
URxvt.perl-ext-common: default,url-select,resize-font
URxvt.perl-ext: default,url-select,resize-font
URxvt.keysym.M-u: perl:url-select:select_next
URxvt.url-select.launcher: /usr/bin/xdg-open
URxvt.url-select.underline: true
URxvt.resize-font.smaller: C-Down
URxvt.resize-font.bigger: C-Up
URxvt.resize-font.reset: C-Left

           !State:           'bg',     'fg',     'bgalt',  'hlbg',   'hlfg'
           rofi.color-normal: #000000,  #ffffff,  #000000,  #9f3aed,  #ffffff
           rofi.color-urgent: #9f3aed,  #ffffff,  #000000,  #9f3aed,  #ffffff
           rofi.color-active: #9f3aed,  #ffffff,  #000000,  #9f3aed,  #ffffff
	   rofi.color-window: #000000, #9f3aed, #000000

! Restore Ctrl+Shift+(c|v)
URxvt.keysym.Shift-Control-V: eval:paste_clipboard
URxvt.keysym.Shift-Control-C: eval:selection_to_clipboard
URxvt.iso14755: false
URxvt.iso14755_52: false
